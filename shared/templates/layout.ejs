                </div>
                <!-- / .sidebar-panel -->
                <!-- .side-nav -->
                <div class="side-nav">
                    <ul class="nav">
                        <li>
                            <a href="/manager"<%-page==="manager"?` class="active"`:``; %>>
                                <i>
                                    <svg class="custom-icon" style="font-size: 30px;">
                                        <use xlink:href="#icon-sim_cards"></use>
                                    </svg>
                                </i>
                                <span class="txt">SIMs</span>
                            </a>
                        </li>
                        <li>
                            <a href="/webphone"<%-page==="webphone"?` class="active"`:``; %>>
                                <i>
                                    <svg class="custom-icon" style="font-size: 30px;">
                                        <use xlink:href="#icon-webphone"></use>
                                    </svg>
                                </i>
                                <span class="txt">Web phone</span>
                            </a>
                        </li>
                        <li>
                            <a href="/subscription"<%-page==="subscription"?` class="active"`:``; %>>
                                <i>
                                    <svg class="custom-icon" style="font-size: 30px;">
                                        <use xlink:href="#icon-subscription"></use>
                                    </svg>
                                </i>
                                <span class="txt">Payments</span>
