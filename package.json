{
  "name": "semasim-frontend",
  "version": "0.1.0",
  "scripts": {
    "prepare": "npm run install-all && npm run build",
    "tsc": "node ./api/node_modules/typescript/bin/tsc --declaration ./index.ts",
    "install-all": "npm run i-shared && npm run i-login && npm run i-register && npm run i-manager && npm run i-webphone",
    "i-shared": "cd shared && npm install",
    "i-login": "cd pages/login && npm install",
    "i-register": "cd pages/register && npm install",
    "i-manager": "cd pages/manager && npm install",
    "i-webphone": "cd pages/webphone && npm install",
    "build": "npm run b-shared && npm run b-login && npm run b-register && npm run b-manager && npm run b-webphone",
    "b-shared": "cd shared && npm run tsc",
    "b-login": "cd pages/login && npm run build",
    "b-register": "cd pages/register && npm run build",
    "b-manager": "cd pages/manager && npm run build",
    "b-webphone": "cd pages/webphone && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garronej/semasim-frontend.git"
  },
  "author": "Joseph Garrone",
  "license": "SEE LICENSE IN LICENSE"
}
