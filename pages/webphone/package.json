{
  "name": "webphone-page",
  "version": "0.1.0",
  "main": "./dist/webphone.js",
  "scripts": {
    "build": "npm run tsc && npm run browserify",
    "tsc": "node ./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "browserify": "node ./node_modules/browserify/bin/cmd . -o ../../static/webphone.js",
    "watchify": "node ./node_modules/watchify/bin/cmd . -o ../../static/webphone.js",
    "tsc-w": "node ./node_modules/typescript/bin/tsc -w -p ./tsconfig.json",
    "watch": "node node_modules/concurrently/src/main --kill-others \"npm run tsc-w\" \"npm run watchify\""
  },
  "browserify": {
    "transform": [
      "html2js-browserify",
      "lessify"
    ]
  },
  "dependencies": {
    "moment": "^2.20.1",
    "sdp-transform": "^2.3.1",
    "ts-events-extended": "github:garronej/ts-events-extended"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "@types/autosize": "^3.0.6",
    "@types/bootstrap": "3.3.28",
    "@types/icheck": "^0.8.29",
    "@types/intl-tel-input": "0.0.8",
    "@types/jquery": "2.0.48",
    "@types/jquery.slimscroll": "^1.3.31",
    "@types/jquery.validation": "1.13.27",
    "@types/jqueryui": "1.9.27",
    "browserify": "^14.4.0",
    "html2js-browserify": "^1.3.0",
    "lessify": "^1.0.1",
    "typescript": "^2.7.1",
    "watchify": "^3.9.0"
  }
}
