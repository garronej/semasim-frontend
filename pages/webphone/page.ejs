const version= "0.1.16";
%>

<!doctype html>
<!--[if lt IE 8]><html class="no-js lt-ie8"> <![endif]-->
<html class="no-js">

<head>
    <%- include("/head_common.ejs", { "title": "Webphone" }); %>

    <!-- Css files -->
    <%- include("/style_common.ejs"); %>

    <!-- intlTelInput -->
    <link rel="stylesheet" href="<%-assets_root%>plugins/ui/intl-tel-input/css/intlTelInput.css">
    <%- include("/intl_tel_input_style_extra.ejs"); %>

    <%- include("/favicon.ejs"); %>

</head>

<body>
    <%- include("/outdated.ejs"); %>

    <%- include("/layout.ejs", { "section": "top", "page": "webphone" }); %>

    <div class="row">
        <div id="page-payload" class="col-lg-11">

            <h2>Loading...</h2>

        </div>
    </div>

    <%- include("/layout.ejs", { "section": "bottom" }); %>

    <!-- Back to top -->
    <div id="back-to-top">
        <a href="#">Back to Top</a>
    </div>

    <%- include("/custom_icons.ejs"); %>

    <!-- Javascripts -->

    <%- include("/promise_polyfill.ejs"); %>

    <%- include("/debug.ejs"); %>

    <%- include("/include_jquery.ejs"); %>

    <!-- Bootstrap plugins -->
    <script src="<%-assets_root%>js/bootstrap/bootstrap.min.js"></script>

    <!-- Core plugins ( not remove ) -->
    <script src="<%-assets_root%>js/libs/modernizr.custom.js"></script>
    <!-- Handle responsive view functions -->
    <script src="<%-assets_root%>js/jRespond.min.js"></script>
    <!-- Custom scroll for sidebars,tables and etc. -->
    <script src="<%-assets_root%>plugins/core/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="<%-assets_root%>plugins/core/slimscroll/jquery.slimscroll.horizontal.min.js"></script>
    <!-- Remove click delay in touch -->
    <script src="<%-assets_root%>plugins/core/fastclick/fastclick.js"></script>
    <!-- Increase jquery animation speed -->
    <script src="<%-assets_root%>plugins/core/velocity/jquery.velocity.min.js"></script>
    <!-- Quick search plugin (fast search for many widgets) -->
    <script src="<%-assets_root%>plugins/core/quicksearch/jquery.quicksearch.js"></script>
    <!-- Bootbox fast bootstrap modals -->
    <script src="<%-assets_root%>plugins/ui/bootbox/bootbox.min.js"></script>
    <script src="<%-assets_root%>js/jquery.dynamic.js"></script>
    <script src="<%-assets_root%>js/main.js"></script>
    
    <!-- jsSIP -->
    <%- include("/jssip.ejs"); %>
    
    <!-- intlTelInput -->
    <script src="<%-assets_root%>plugins/ui/intl-tel-input/js/utils.js"></script>
    <script src="<%-assets_root%>plugins/ui/intl-tel-input/js/intlTelInput.min.js"></script>
    
    <!-- jquery-validation -->
    <script src="<%-assets_root%>plugins/forms/validation/jquery.validate.min.js"></script>
    <script src="<%-assets_root%>plugins/forms/validation/additional-methods.min.js"></script>
    
    <!-- jquery-autosize -->
    <script src="<%-assets_root%>plugins/forms/autosize/jquery.autosize.min.js"></script>
    
    <!-- title-notifier -->
    <script src="<%-assets_root%>plugins/ui/title-notifier/title_notifier.js"></script>

    <!-- ion-sound -->
    <script src="<%-assets_root%>plugins/misc/ion-sound/js/ion.sound<%-!isDevEnv && !isWebView ?'.min':'';%>.js"></script>

    <script>
        ion.sound({
            "sounds": [
                { "name": "water_droplet" },
                { "name": "button_tiny" },
                { "name": "semasim_ringtone" },
                { "name": "loading" }
            ],
            "volume": 0.5,
            "path": "<%-assets_root%>plugins/misc/ion-sound/sounds/",
            "preload": true
        });
    </script>
    
    <!-- timer.jquery @ walmik/timer.jquery -->
    <script src="<%-assets_root%>js/libs/timer_jquery_v0.7.0/timer.jquery.js"></script>
    
    <!-- Page code -->
    <%- include("/page_script.ejs", {"page":"webphone", version }); %>

